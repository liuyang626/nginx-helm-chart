apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx
  labels:
    app: nginx
spec:
  replicas: 1
  selector:
    matchLabels:
      app: nginx
  template:
    metadata:
      labels:
        app: nginx
    spec:
      containers:
      - name: nginx
        image: #{ydm-test.deploy.imageHost}#:#{ydm-test.deploy.imageTag}#
        ports:
        - containerPort: 80
        resources:
          limits:
            cpu: "500m"
      imagePullSecrets:
      - name: acr-regsecret
